search: go run main.go server --surveyor-address tcp://localhost:40999
websearch: go run main.go client --respondent-name websearch --respondent-surveyor tcp://localhost:40999
videosearch: go run main.go client --respondent-name videosearch --respondent-surveyor tcp://localhost:40999
imagesearch: go run main.go client --respondent-name imagesearch --respondent-surveyor tcp://localhost:40999
